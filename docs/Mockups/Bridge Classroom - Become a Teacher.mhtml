        <button class=3D"modal-btn secondary">=E2=86=90 Back</button>
        <button class=3D"modal-btn primary" id=3D"activateBtn" disabled=3D"=
">Activate Teacher Account</button>
      </div>
    </div>

    <!-- =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D -->
    <!-- STEP 3: SUCCESS          -->
    <!-- =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
